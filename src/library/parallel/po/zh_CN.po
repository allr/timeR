#: fork.c:135 fork.c:138
msgid "unable to create a pipe"
msgstr "无法建立管道"

#: fork.c:149
#, c-format
msgid "unable to fork, possible reason: %s"
msgstr "无法分叉，可能的原因是：%s"

#: fork.c:178
msgid "memory allocation error"
msgstr "内存分配错误"

#: fork.c:216
msgid "only children can send data to the master process"
msgstr "只有子进程才能给主进程发送数据"

#: fork.c:218
msgid "there is no pipe to the master process"
msgstr "有没可以接到主进程的管道"

#: fork.c:220
msgid "content to send must be RAW, use serialize() if needed"
msgstr "发送的内容必须是原始的，如果需要的话请用serialize（）"

#: fork.c:229 fork.c:236 fork.c:566
msgid "write error, closing pipe to the master"
msgstr "书写错误，请关闭通往主进程的管道"

#: fork.c:249
msgid "only the master process can send data to a child process"
msgstr "只有主进程才能给子进程发送数据"

#: fork.c:256
#, c-format
msgid "child %d does not exist"
msgstr "不存在%d这个子进程"

#: fork.c:262
msgid "write error"
msgstr "书写错误"

#: fork.c:342 fork.c:464
#, c-format
msgid "error '%s' in select"
msgstr "选择时出了'%s'这样一个错"

#: fork.c:548
msgid "'mckill' failed"
msgstr "'mckill'失败"

#: fork.c:558
msgid "'mcexit' can only be used in a child process"
msgstr "'mcexit'只能用于子进程"

#: fork.c:579
msgid "'mcexit' failed"
msgstr "'mcexit'失败"

#: fork.c:601 fork.c:610
msgid "invalid CPU affinity specification"
msgstr "CPU亲和力规范不对"

#: fork.c:621
msgid "requested CPU set is too large for this system"
msgstr "所需要的CPU设置对于这个系统来说太大了"

#: fork.c:644
msgid "retrieving CPU affinity set failed"
msgstr "无法收取CPU亲和力"
